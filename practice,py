print (2**3) # 8
print (5%3) # 나머지 구하기 2 
print (10%3) # 나머지 1
print (5//3) #1 몫 구하기
print (10//3) #3 몫 구하기

print (13 > 4) # true
print ( 4 <= 5)

print ( 3==3 )
print ( 2*3 == 2+4)

print ( 4==5 ) # false

print ( 1 != 3 ) # 등호 간에 같지 않다 불리안
print ( not ( 1 != 2-1 ) )
print ( (3 + 3 < 10) and (2 - 1 < 3) )

print ( 5 > 3 > 2)

